{
  "name": "ocrScanner",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "npx react-native run-ios",
    "ios:device": "npx react-native run-ios --device",
    "lint": "eslint .",
    "install:pods": "cd ios && pod install",
    "start": "react-native start",
    "ios:14": "react-native run-ios --simulator=\"iPhone 14\"",
    "ios:Xr": "react-native run-ios --simulator=\"iPhone Xr\"",
    "ios:plus:14": "react-native run-ios --simulator=\"iPhone 14 Plus\"",
    "ios:pro:14": "react-native run-ios --simulator=\"iPhone 14 Pro\"",
    "ios:pro:11": "react-native run-ios --simulator=\"iPad Pro (11-inch) (3rd generation)\"",
    "ios:13:mini": "react-native run-ios --simulator=\"iPhone 13 mini\"",
    "test": "jest"
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "moment": "^2.29.4",
    "react": "18.2.0",
    "react-native": "0.71.8",
    "react-native-reanimated": "^2.17.0",
    "react-native-safe-area-context": "^4.5.3",
    "react-native-screens": "^3.20.0",
    "react-native-svg": "^13.9.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-vision-camera": "^2.15.4",
    "vision-camera-ocr": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-native-vector-icons": "^6.4.13",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.9",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
